State)\n    const [login, setLogin] = useState(true)\n\n    const handleChange = (e) => {\n        setUser({\n            ...user,\n            [e.target.name]: e.target.value\n        })\n    }\n\n    // const onSubmit = (e) => {\n    //     e.preventDefault()\n    //     if(login){\n    //         auth.signInWithEmailAndPassword(user.email,user.password)\n    //             .then(setUser(initialState))\n    //             .then(history.push('/dashboard'))\n    //             .catch(err => alert(err.message))\n    //     } else {\n    //         auth.createUserWithEmailAndPassword(user.email,user.password)\n    //             .then(setUser(initialState))\n    //             .then(history.push('/dashboard'))\n    //             .catch(err => alert(err.message))\n    //     }\n    // }\n\n\n    //WITH AXIOS\n\n    const onSubmit = (e) => {\n        e.preventDefault()\n        if(login){\n            axios.post('https://water-my-plants-tt101.herokuapp.com/users/login', user)\n            .then(res => {\n                localStorage.setItem('userToken', res.data.token)\n                setUser(initialState)\n            })\n            .then(history.push('/dashboard'))\n        } else {\n            axios.post('https://water-my-plants-tt101.herokuapp.com/users/register', user)\n            .then(res => {\n                localStorage.setItem('userToken', res.data.token);\n                setLogin(!login)\n            })\n            .then(history.push('/login'))\n\n        }\n    }\n\n\n    return (\n        <>\n        <Form onSubmit={onSubmit}>\n            <h2>{login ? 'Login' : 'Sign Up'}</h2> \n            <TextField\n                type='text'\n                name='username'\n                value={user.username}\n                onChange={handleChange} \n                variant='outlined'\n                label='username'\n                margin='dense'\n            />\n            <TextField\n                name='email'\n                type='email'\n                value={user.email}\n                onChange={handleChange} \n                variant='outlined'\n                label='email'\n                margin='dense'\n            />\n            <TextField\n                name='password'\n                type='password'\n                value={user.password}\n                onChange={handleChange} \n                variant='outlined'\n                label='password'\n                margin='dense'\n            />\n            <Button\n                type='submit' \n                variant='contained'\n                color='primary'\n            >\n                {login ? 'Login' : 'Sign Up'}\n            </Button>\n        </Form>\n        <Button color='secondary' size='small' variant='contained' onClick={()=> setLogin(!login)}>{login ? 'Go to Sign Up' : 'Go to Log In'}</Button>\n        </>\n    )\n}\n\nexport default LoginForm\n\nconst Form = styled.form`\n    display: grid;\n    grid-template-columns: 1fr;\n    border: 1px solid #ccc;\n    border-radius: 5px;\n    min-width: 250px;\n    max-width: 300px;\n    padding: 1rem;\n    gap: .25rem;\n    background-color: #fff;\n    box-shadow: 0 0 3px rgba(0,0,0,.35);\n    & h2{\n        text-align: center;\n    }\n`","/Users/petevs/Documents/Lambda/unit-3-build/front-end/client/src/components/PlantCard.js",[],"/Users/petevs/Documents/Lambda/unit-3-build/front-end/client/src/utils/axiosWithAuth.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":1,"column":17,"nodeType":"86","messageId":"87","endLine":1,"endColumn":27},{"ruleId":"84","severity":1,"message":"88","line":1,"column":29,"nodeType":"86","messageId":"87","endLine":1,"endColumn":37},{"ruleId":"84","severity":1,"message":"89","line":2,"column":8,"nodeType":"86","messageId":"87","endLine":2,"endColumn":20},{"ruleId":"84","severity":1,"message":"90","line":3,"column":8,"nodeType":"86","messageId":"87","endLine":3,"endColumn":16},{"r